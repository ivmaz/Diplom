<h1>Список Типой Мероприятий</h1>

<table>
  <tr>
    <th>Название</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @types.each do |type| %>
  <tr>
    <td><%= type.name %></td>
    <td><%= link_to 'Показать', type %></td>
    <td><%= link_to 'Редактировать', edit_type_path(type) %></td>
    <td><%= link_to 'Удалить', type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

